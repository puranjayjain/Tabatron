{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    },
    "default_title": "tabatron",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "globals.js",
      "storageHandler.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "topSites",
    "chrome://favicon/",
    "storage",
    "webNavigation"
  ],
  "offline_enabled": true
}
